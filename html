        // Theme toggle
        function toggleTheme() {
            const body = document.body;
            const themeIcon = document.getElementById('themeIcon');
            const themeText = document.getElementById('themeText');
            
            body.classList.toggle('light-theme');
            
            if (body.classList.contains('light-theme')) {
                themeIcon.textContent = 'üåô';
                themeText.textContent = 'Dark Mode';
                localStorage.setItem('theme', 'light');
            } else {
                themeIcon.textContent = '‚òÄÔ∏è';
                themeText.textContent = 'Light Mode';
                localStorage.setItem('theme', 'dark');
            }

            // Update chart defaults and recreate charts
            updateChartDefaults();
            recreateAllCharts();
        }

        // Load saved theme on page load
        window.addEventListener('DOMContentLoaded', function() {
            const savedTheme = localStorage.getItem('theme');
            if (savedTheme === 'light') {
                document.body.classList.add('light-theme');
                document.getElementById('themeIcon').textContent = 'üåô';
                document.getElementById('themeText').textContent = 'Dark Mode';
                updateChartDefaults();
            }

            // Initialize stats
            document.getElementById('stat-participants').textContent = dashboardData.overview.totalParticipants;
            document.getElementById('stat-criteria').textContent = dashboardData.overview.frameworkCriteria;
            document.getElementById('stat-reviews').textContent = dashboardData.overview.committeeReviews;
            document.getElementById('framework-must').textContent = dashboardData.framework.byCategory['Must Have'];
            document.getElementById('framework-prefer').textContent = dashboardData.framework.byCategory['Strongly Prefer'];
            document.getElementById('framework-flags').textContent = dashboardData.framework.byCategory['Red Flag'];

            // Create all charts
            recreateAllCharts();
        });
    </script>
</body>
</html>
