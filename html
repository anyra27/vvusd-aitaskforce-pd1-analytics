        // Store all chart instances globally for theme updates
        let chartInstances = {};

        // Chart.js default configuration
        Chart.defaults.color = 'rgba(255, 255, 255, 0.8)';
        Chart.defaults.borderColor = 'rgba(255, 255, 255, 0.1)';
        Chart.defaults.font.family = '-apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, sans-serif';
        Chart.defaults.font.size = 11;

        // Update Chart.js colors for light theme
        function updateChartDefaults() {
            const isLight = document.body.classList.contains('light-theme');
            Chart.defaults.color = isLight ? 'rgba(15, 23, 42, 0.8)' : 'rgba(255, 255, 255, 0.8)';
            Chart.defaults.borderColor = isLight ? 'rgba(15, 23, 42, 0.1)' : 'rgba(255, 255, 255, 0.1)';
        }

        // VVUSD color palette with theme-aware colors
        function getChartColors() {
            const isLight = document.body.classList.contains('light-theme');
            if (isLight) {
                return {
                    darkBlue: 'rgba(15, 35, 65, 0.9)',
                    lightBlue: 'rgba(30, 64, 175, 0.85)',
                    mediumBlue: 'rgba(29, 78, 216, 0.85)',
                    cyan: 'rgba(8, 145, 178, 0.85)',
                    teal: 'rgba(13, 148, 136, 0.85)',
                    green: 'rgba(5, 150, 105, 0.85)',
                    darkGreen: 'rgba(22, 163, 74, 0.85)',
                    lime: 'rgba(77, 124, 15, 0.85)',
                    brightGreen: 'rgba(101, 163, 13, 0.85)'
                };
            } else {
                return {
                    darkBlue: 'rgba(23, 52, 98, 0.85)',
                    lightBlue: 'rgba(161, 197, 214, 0.85)',
                    mediumBlue: 'rgba(37, 150, 190, 0.85)',
                    cyan: 'rgba(6, 182, 212, 0.85)',
                    teal: 'rgba(20, 184, 166, 0.85)',
                    green: 'rgba(16, 185, 129, 0.85)',
                    darkGreen: 'rgba(34, 197, 94, 0.85)',
                    lime: 'rgba(101, 163, 13, 0.85)',
                    brightGreen: 'rgba(132, 204, 22, 0.85)'
                };
            }
        }

        function getMultiColorPalette() {
            const colors = getChartColors();
            return [
                colors.darkBlue, colors.lightBlue, colors.mediumBlue,
                colors.cyan, colors.teal, colors.green,
                colors.darkGreen, colors.lime, colors.brightGreen
            ];
        }

        // Tab switching
        function showTab(tabName) {
            document.querySelectorAll('.tab-content').forEach(tab => tab.classList.remove('active'));
            document.querySelectorAll('.tab-btn').forEach(btn => btn.classList.remove('active'));
            document.getElementById(tabName).classList.add('active');
            event.target.classList.add('active');
            window.scrollTo({ top: 0, behavior: 'smooth' });
        }

        // Recreate all charts with new colors
        function recreateAllCharts() {
            const colors = getChartColors();
            const multiColors = getMultiColorPalette();

            // Destroy existing charts
            Object.values(chartInstances).forEach(chart => chart.destroy());
            chartInstances = {};

            // Overview Charts
            chartInstances.participation = new Chart(document.getElementById('participationChart'), {
                type: 'bar',
                data: {
                    labels: ['Session Feedback', 'Framework Criteria', 'Committee Reviews', 'Resource Audit'],
                    datasets: [{
                        label: 'Submissions',
                        data: [
                            dashboardData.overview.totalParticipants,
                            dashboardData.overview.frameworkCriteria,
                            dashboardData.overview.committeeReviews,
                            dashboardData.overview.resourceAudits
                        ],
                        backgroundColor: multiColors
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: true,
                    plugins: { legend: { display: false } },
                    scales: { y: { beginAtZero: true } }
                }
            });

            chartInstances.workshopRating = new Chart(document.getElementById('workshopRatingChart'), {
                type: 'doughnut',
                data: {
                    labels: Object.keys(dashboardData.feedback.workshopModel),
                    datasets: [{
                        data: Object.values(dashboardData.feedback.workshopModel),
                        backgroundColor: multiColors
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: true,
                    plugins: { legend: { position: 'bottom' } }
                }
            });

            // Feedback Charts
            chartInstances.workedWell = new Chart(document.getElementById('workedWellChart'), {
                type: 'bar',
                data: {
                    labels: Object.keys(dashboardData.feedback.whatWorkedWell),
                    datasets: [{
                        label: 'Participants',
                        data: Object.values(dashboardData.feedback.whatWorkedWell),
                        backgroundColor: colors.teal
                    }]
                },
                options: {
                    indexAxis: 'y',
                    responsive: true,
                    maintainAspectRatio: true,
                    plugins: { legend: { display: false } },
                    scales: { x: { beginAtZero: true } }
                }
            });

            chartInstances.needsAdjustment = new Chart(document.getElementById('needsAdjustmentChart'), {
                type: 'bar',
                data: {
                    labels: Object.keys(dashboardData.feedback.needsAdjustment),
                    datasets: [{
                        label: 'Participants',
                        data: Object.values(dashboardData.feedback.needsAdjustment),
                        backgroundColor: colors.mediumBlue
                    }]
                },
                options: {
                    indexAxis: 'y',
                    responsive: true,
                    maintainAspectRatio: true,
                    plugins: { legend: { display: false } },
                    scales: { x: { beginAtZero: true } }
                }
            });

            // Comfort Levels Chart
            const comfortLabels = ['AI Capabilities', 'Evaluating Tools', 'Building Apps', 'Policy Implications', 'Google AI Suite', 'Brisk AI'];
            const comfortData = [
                dashboardData.feedback.comfortLevels.aiCapabilities,
                dashboardData.feedback.comfortLevels.evaluating,
                dashboardData.feedback.comfortLevels.building,
                dashboardData.feedback.comfortLevels.policy,
                dashboardData.feedback.comfortLevels.googleAI,
                dashboardData.feedback.comfortLevels.briskAI
            ];

            const avgComfort = comfortData.map(levels => {
                let total = 0, count = 0;
                Object.entries(levels).forEach(([rating, num]) => {
                    total += parseInt(rating) * num;
                    count += num;
                });
                return count > 0 ? (total / count).toFixed(2) : 0;
            });

            chartInstances.comfortLevels = new Chart(document.getElementById('comfortLevelsChart'), {
                type: 'bar',
                data: {
                    labels: comfortLabels,
                    datasets: [{
                        label: 'Average Comfort Level',
                        data: avgComfort,
                        backgroundColor: colors.lightBlue
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: true,
                    plugins: { legend: { display: false } },
                    scales: {
                        y: { beginAtZero: true, max: 5, ticks: { stepSize: 1 } }
                    }
                }
            });

            chartInstances.focusAreas = new Chart(document.getElementById('focusAreasChart'), {
                type: 'bar',
                data: {
                    labels: Object.keys(dashboardData.feedback.focusAreas),
                    datasets: [{
                        label: 'Participants',
                        data: Object.values(dashboardData.feedback.focusAreas),
                        backgroundColor: colors.cyan
                    }]
                },
                options: {
                    indexAxis: 'y',
                    responsive: true,
                    maintainAspectRatio: true,
                    plugins: { legend: { display: false } },
                    scales: { x: { beginAtZero: true } }
                }
            });

            // Framework Charts
            chartInstances.frameworkCategory = new Chart(document.getElementById('frameworkCategoryChart'), {
                type: 'doughnut',
                data: {
                    labels: Object.keys(dashboardData.framework.byCategory),
                    datasets: [{
                        data: Object.values(dashboardData.framework.byCategory),
                        backgroundColor: [colors.darkBlue, colors.teal, colors.green]
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: true,
                    plugins: { legend: { position: 'bottom' } }
                }
            });

            chartInstances.frameworkGrade = new Chart(document.getElementById('frameworkGradeChart'), {
                type: 'bar',
                data: {
                    labels: Object.keys(dashboardData.framework.byGradeSpan),
                    datasets: [{
                        label: 'Criteria Submitted',
                        data: Object.values(dashboardData.framework.byGradeSpan),
                        backgroundColor: [colors.mediumBlue, colors.teal, colors.lightBlue]
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: true,
                    plugins: { legend: { display: false } },
                    scales: { y: { beginAtZero: true } }
                }
            });

            chartInstances.frameworkTags = new Chart(document.getElementById('frameworkTagsChart'), {
                type: 'bar',
                data: {
                    labels: ['Protects Data', 'Encourages Learning', 'Benefits Teachers', 'Promotes Equity', 'Transparent', 'Implementable', 'Cost Effective', 'Values Aligned'],
                    datasets: [{
                        label: 'Times Selected',
                        data: Object.values(dashboardData.framework.tags),
                        backgroundColor: multiColors
                    }]
                },
                options: {
                    indexAxis: 'y',
                    responsive: true,
                    maintainAspectRatio: true,
                    plugins: { legend: { display: false } },
                    scales: { x: { beginAtZero: true } }
                }
            });

            // Committee Review Charts
            chartInstances.tierDecisions = new Chart(document.getElementById('tierDecisionsChart'), {
                type: 'bar',
                data: {
                    labels: Object.keys(dashboardData.reviews.tierDecisions).map(k => k.replace('Tier ', 'T')),
                    datasets: [{
                        label: 'Reviews',
                        data: Object.values(dashboardData.reviews.tierDecisions),
                        backgroundColor: [colors.green, colors.teal, colors.lightBlue, colors.darkBlue]
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: true,
                    plugins: { legend: { display: false } },
                    scales: { y: { beginAtZero: true } }
                }
            });

            chartInstances.roleParticipation = new Chart(document.getElementById('roleParticipationChart'), {
                type: 'doughnut',
                data: {
                    labels: Object.keys(dashboardData.reviews.byRole),
                    datasets: [{
                        data: Object.values(dashboardData.reviews.byRole),
                        backgroundColor: multiColors
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: true,
                    plugins: { legend: { position: 'bottom' } }
                }
            });

            chartInstances.criteriaInfluence = new Chart(document.getElementById('criteriaInfluenceChart'), {
